name: FacebookExt

platforms:
    armv7-android:
        aapt:
            extra-packages: ['com.facebook']

    armv7-ios:
        context:
            frameworks: ['Security']
            weakFrameworks: ['Accounts', 'Social', 'AdSupport']

    arm64-ios:
        context:
            frameworks: ['Security']
            weakFrameworks: ['Accounts', 'Social', 'AdSupport']

